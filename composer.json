{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "exo/teamspeak": "^1.0@dev",
        "guzzlehttp/guzzle": "^7.0.1",
        "guzzlehttp/psr7": "^2.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "lab404/laravel-impersonate": "1.7.7",
        "laravel/framework": "^12.0",
        "laravel/reverb": "*",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "multitheftauto/mtasa-php-sdk": "dev-master",
        "orchestra/parser": "^10.0",
        "owen-it/laravel-auditing": "^14.0",
        "php-http/curl-client": "^2.0",
        "php-http/guzzle7-adapter": "^0.1",
        "php-http/message": "^1.8",
        "predis/predis": "^1.1",
        "sofort/sofortlib-php": "^3.3",
        "tray-labs/laravel-influxdb": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "*",
        "phpunit/phpunit": "^12.0",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/exo/teamspeak",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
